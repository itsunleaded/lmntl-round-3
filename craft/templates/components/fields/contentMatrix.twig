{% set content = element %}

    {% if content.count() %}
        {% for block in content.all() %}
            {% switch block.type %}

            {% case "secondaryHeader" %}
                {% include 'components/_secondaryHeader.twig' with {block: block} only %}
            {% case "sectionTitleSubhead" %}
                {% include 'components/_sectionTitleSubhead.twig' with {block: block} only %}
            {% case "singleColumn" %}
                {% include 'components/_singleColumn.twig' with {block: block} only %}
            {% case "sectionTitleSubheadLeft" %}
                {% include 'components/_sectionTitleSubheadLeft.twig' with {block: block} only %}
            {% case "sectionTwoColumnImageBackground" %}
                {% include 'components/_sectionTwoColumnImageBackground.twig' with {block: block} only %}
            {% case "homeTwoColumnImageBackgroundLeft" %}
                {% include 'components/_homeTwoColumnImageBackgroundLeft.twig' with {block: block} only %}
            {% case "whyTitleSubhead" %}
                {% include 'components/_whyTitleSubhead.twig' with {block: block} only %}
            {% case "aboutOils" %}
                {% include 'components/_aboutOils.twig' with {block: block} only %}
            {% case "powerOfCrystals" %}
                {% include 'components/_powerOfCrystals.twig' with {block: block} only %}
            {% case "sectionSingleColumn" %}
                {% include 'components/fields/_sectionSingleColumn.twig' with {block: block } only %}    


            {% endswitch %}
        {% endfor %}
    {% endif %}
